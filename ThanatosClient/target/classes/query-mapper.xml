<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thanatos.model.Users">
    <select id="selectAgency" resultType="int">
        select ID, NAME from AGENCY where
        id = #{id}
    </select>

    <update id="test" parameterType="java.util.Map">
        Update Users SET Username=#{currentName}
    </update>
    
    <update id="seedAgency">
        INSERT INTO AGENCY VALUES (10,'American family insurance')
    </update>
</mapper>